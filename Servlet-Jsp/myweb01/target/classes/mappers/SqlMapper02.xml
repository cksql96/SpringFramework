<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zerock.myweb01.SqlMapper02">

	<select 
    id="getNow2"
    parameterType="com.zerock.myweb01.TTT.TTTDTO"
    resultType="com.zerock.myweb01.TTT.TTTVO">
	
	<![CDATA[
		SELECT
			*
	    FROM
	        emp
	    WHERE
	    	empno >= ${empno} AND
	    	sal > ${sal}
	]]>
    
	</select>
	
</mapper>
